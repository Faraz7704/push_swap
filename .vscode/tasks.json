{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "*.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "${fileDirname}/../lib/libft/libft.a",
                "${fileDirname}/../lib/ft_printf/libft_printf.a",
                "-I${fileDirname}/../includes/",
                "-I${fileDirname}/../lib/libft/includes",
                "-I${fileDirname}/../lib/ft_printf/includes"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}